{
  "name": "bluekai",
  "version": "1.0.0",
  "description": "BlueSky client for KaiOS 2.5",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack serve --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:e2e": "webpack --config webpack.e2e.config.js",
    "build:analyze": "webpack --mode production --config webpack.config.js --env analyze",
    "analyze": "node analyze-bundle.js",
    "check-size": "node check-bundle-size.js",
    "verify-build": "node verify-production-build.js",
    "serve": "npx serve dist -p 8080",
    "test:build": "npm run build:prod && npm run verify-build",
    "test:e2e": "npm run build:e2e && echo 'E2E bundle built. Open test-e2e-integration.html in browser.'"
  },
  "keywords": [
    "bluesky",
    "kaios",
    "preact"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "preact": "^10.19.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@iconify/iconify": "^3.1.1",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.1",
    "core-js": "^3.34.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.6.1",
    "html-webpack-plugin": "^5.5.4",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
